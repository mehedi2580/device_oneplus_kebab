dontaudit {
  hal_graphics_allocator_default
  hal_sensors_default
  platform_app
  bootanim
  system_app
  surfaceflinger
  untrusted_app_29
  vendor_init-qti-dcvs-sh
  vendor_sensors
  init
  priv_app
  gmscore_app
  hal_bootctl_default
  hal_fingerprint
  } {
  bt_firmware_file
  firmware_file
  mnt_vendor_file
  gsi_metadata_file
  storage_file
  sysfs_msm_subsys
  vendor_wcnss_service
}:dir r_dir_perms;

dontaudit {
  gmscore_app
  hal_camera_default
  hal_audio_default
  vendor_hal_neuralnetworks_default
  vendor_qti_init_shell
  hal_fingerprint_default
  vendor_init
  untrusted_app
  } { 
  default_prop
  property_type 
  vendor_sysfs_soc
  wifi_hal_prop
}:file r_file_perms;

dontaudit {
    hal_atrace_default
    traced_probes
    hal_audio_default
    hal_sensors_default
    vendor_adpl
    vendor_cnd
    vendor_dpmd
    vendor_hal_imsrtp
    vendor_hal_rcsservice
    vendor_ims
    vendor_netmgrd
    vendor_qti
    vendor_sensors_qti
    } {
    vendor_diag_device
    debugfs_tracing_debug
}:{ chr_file file } rw_file_perms;

dontaudit hal_wifi_default self:capability { kill sys_admin };
dontaudit hal_bootctl_default self:capability { kill sys_admin };
dontaudit vendor_mdm_helper self:capability { kill sys_admin };
dontaudit vendor_hal_iop_default self:capability { kill sys_admin };
